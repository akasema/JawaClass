package com.Class9;

public class Trials {

	public static void main(String[] args) {
		// TODO Auto-generated method stub
		
		for (int i=1; i<6; i++){
		      for (int j=1; j<6; j++){
		        if (6-j>i){
		          System.out.print(" ");
		        }else {
		          System.out.print(i);
		        }
		      }
		      System.out.println();
		        }
		    
		for (int i=1; i<=5; i++){
		      for (int j=1; j<=5; j++){
		    	  if (j==1 || j==5) {
		    	System.out.print("$");
		    	      }else if (j==2 || j==3 || j==4){
		    	    	  System.out.println(" ");
		    	      }
		      
		    	    }
		      System.out.println();
		    	  }
		    }
	
}

