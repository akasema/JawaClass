package com.Class11;

public class Recap {

	public static void main(String[] args) {
		// single dimension Array
// How we can express the arrays
		
//		int[] h=new int[3];
//		int e [];
//		int y[], u; // y is array, u is single variable
//		y=new int [3];		
//		u= 12 ; 
		
		String [] car=new String[6];
		car[0]="Mercedes";
		car[1]="Masarati";
		car[2]="Honda";
		car[3]="Mazda";
		car[4]="Nissan";
		car[5]="Toyota";
		
		for (int i=0; i<=5; i++){
		
		System.out.println(car[i]);
	}
	}
}
