package jawaRewievClasses;

public class Tasks {

	public static void main(String[] args) {
		// TODO Auto-generated method stub
		
		int i = 50;

		int[] a = new int[10];
		System.out.println(a.length);

		a = new int[100];
		System.out.println(a.length);

		a = new int[i];
		System.out.println(a.length);
		}}
	}

}
