package com.class3;

public class PositiveNegative {
	public static void main(String[]args ) {
		int num1=2;
		
		if (num1<0) {
			System.out.println("This Number is Negative");
		}else if (num1==0) {
			System.out.println("This Number is Zero");
			
		}else  {
			System.out.println("This Number is Positive");
	}

	}
}

