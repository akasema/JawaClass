package com.Class13;

public class RegularExpression {

	public static void main(String[] args) {
		// TODO Auto-generated method stub
		/*
		 * Print only text not numbers
		 */
		
		String str="14234Hello14234Hello14234";
		System.out.println(str.replaceAll("[0-9]", ""));//[] önemli Syaý haricindekileri bas
		System.out.println(str.replaceAll("[a-zA-Z]", ""));// Harf haricindekileri bas
										//Sadece buyuk yada kucuk harfleri basmasýný da saglayabilirsin
System.out.println("============");	
	String a="^+&/%(Hello&^'=?How 653800You";
	System.out.println(a.replaceAll("[a-zA-Z0-9]", ""));// harf ve sayý haricini bas
	System.out.println(a.replaceAll("[^^a-zA-Z0-9]", ""));

	}

}
