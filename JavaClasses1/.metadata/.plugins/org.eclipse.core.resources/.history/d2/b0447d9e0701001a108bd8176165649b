package com.Class14;

public class Two_D_Array_ForEach {

	public static void main(String[] args) {
		// TODO Auto-generated method stub
		
		int [][] intArray= {
				{2,3,4},
				{4,5,6},
				{6,7,8},
				{8,9,0}};
		for (int[] row:intArray) {
			for (int[]col:intArray) {
				System.out.println(col+" ");
			}
			
			
		}

	}

}
